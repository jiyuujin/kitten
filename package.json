{
  "name": "kitten",
  "description": "",
  "scripts": {
    "dev": "gatsby develop --https --cert-file ./certificates/localhost.pem --key-file ./certificates/localhost-key.pem",
    "build": "gatsby build",
    "build:og-gen": "ts-node --project tsconfig.builder.json scripts/og-gen.ts",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "eslint": "eslint ./ --ext ts,tsx",
    "eslint:fix": "eslint ./ --ext ts,tsx --fix",
    "stylelint": "stylelint --config .stylelintrc.yml 'src/**/*.scss'",
    "stylelint:fix": "stylelint --config .stylelintrc.yml 'src/**/*.scss' --fix",
    "test": "jest --testEnvironment jsdom",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@nekohack/footer-ui": "1.0.0",
    "@nekohack/normalize.css": "2.4.1",
    "@stripe/react-stripe-js": "1.7.0",
    "@stripe/stripe-js": "1.22.0",
    "dayjs": "1.10.7",
    "gatsby": "3.14.6",
    "gatsby-plugin-google-adsense": "1.1.3",
    "gatsby-plugin-google-analytics": "3.14.0",
    "gatsby-plugin-image": "2.5.2",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-preact": "6.4.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-react-intl": "3.0.2",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-robots-txt": "1.7.0",
    "gatsby-plugin-sass": "4.14.0",
    "gatsby-plugin-sharp": "3.14.3",
    "gatsby-plugin-typescript": "3.14.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-sharp": "3.14.0",
    "gatsby-transformer-yaml": "3.14.0",
    "nekohack-ui": "3.0.2",
    "preact": "10.6.5",
    "preact-render-to-string": "5.1.19",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-test-renderer": "17.0.2",
    "react-text-loop": "2.3.0",
    "react-tooltip": "4.2.21",
    "styled-components": "5.3.3",
    "ts-node": "10.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/helper-create-regexp-features-plugin": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@nekohack/eslint-config-react": "0.8.0",
    "@swc/core": "1.2.135",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.5",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "babel-jest": "27.4.6",
    "babel-preset-gatsby": "2.4.0",
    "dotenv": "14.3.2",
    "eslint": "8.8.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "puppeteer": "13.1.2",
    "sass": "1.49.0",
    "stylelint": "14.3.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-scss": "4.1.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.(js|jsx)?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules",
      "__tests__/constants.ts",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  }
}
