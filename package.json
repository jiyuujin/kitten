{
  "name": "yuukit.me",
  "description": "",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "lint": "eslint ./ --ext ts,tsx",
    "lint:fix": "eslint ./ --ext ts,tsx --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@nekohack/tiny-components": "1.0.2",
    "dayjs": "1.9.5",
    "gatsby": "2.25.3",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-google-analytics": "2.4.1",
    "gatsby-plugin-manifest": "2.5.2",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sass": "2.4.2",
    "gatsby-plugin-sharp": "2.7.1",
    "gatsby-plugin-typescript": "2.5.0",
    "gatsby-source-filesystem": "2.4.2",
    "gatsby-transformer-sharp": "2.5.21",
    "gatsby-transformer-yaml": "2.4.15",
    "nekohack-ui": "1.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-text-loop": "2.3.0",
    "styled-components": "5.2.1",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.12.1",
    "@nekohack/eslint-config-react": "0.3.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-test-renderer": "16.9.3",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "0.5.16",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "react-test-renderer": "16.14.0",
    "sass": "1.29.0",
    "ts-jest": "26.4.4"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.(js|jsx)?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  }
}
