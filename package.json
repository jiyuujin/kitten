{
  "name": "yuma-kitamura.nekohack.me",
  "description": "",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint ./ --ext ts,tsx",
    "lint:fix": "eslint ./ --ext ts,tsx --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@nekohack/footer-ui": "1.0.0",
    "@nekohack/normalize.css": "2.3.5",
    "@stripe/react-stripe-js": "1.4.1",
    "@stripe/stripe-js": "1.17.1",
    "dayjs": "1.10.6",
    "gatsby": "2.32.13",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-intl": "0.3.3",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-react-helmet": "4.13.0",
    "gatsby-plugin-react-svg": "3.0.1",
    "gatsby-plugin-robots-txt": "1.6.10",
    "gatsby-plugin-sass": "3.2.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-typescript": "2.12.1",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-yaml": "2.11.0",
    "nekohack-ui": "2.9.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-text-loop": "2.3.0",
    "styled-components": "5.3.1",
    "typescript": "4.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/helper-create-regexp-features-plugin": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@nekohack/eslint-config-react": "0.5.0",
    "@types/jest": "26.0.24",
    "@types/react": "16.14.14",
    "@types/react-dom": "16.9.14",
    "@types/react-helmet": "6.1.2",
    "@types/react-test-renderer": "16.9.5",
    "@types/styled-components": "5.1.13",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "1.13.0",
    "dotenv": "8.6.0",
    "eslint": "7.32.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "prettier": "2.3.2",
    "react-test-renderer": "16.14.0",
    "sass": "1.39.0",
    "ts-jest": "26.5.6"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.(js|jsx)?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules",
      "__tests__/constants.ts",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  }
}
