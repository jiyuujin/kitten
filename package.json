{
  "name": "yuukit.me",
  "description": "",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@nekohack/tiny-components": "1.0.2",
    "dayjs": "1.9.1",
    "gatsby": "2.24.67",
    "gatsby-plugin-google-analytics": "2.3.15",
    "gatsby-plugin-manifest": "2.4.33",
    "gatsby-plugin-react-helmet": "3.3.12",
    "gatsby-plugin-sass": "2.3.14",
    "gatsby-plugin-typescript": "2.4.21",
    "gatsby-source-filesystem": "2.3.32",
    "gatsby-transformer-yaml": "2.4.13",
    "nekohack-ui": "1.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-text-loop": "2.3.0",
    "styled-components": "5.2.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.10.4",
    "@nekohack/eslint-config-react": "0.3.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-test-renderer": "16.9.3",
    "@types/styled-components": "5.1.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.3.0",
    "babel-preset-gatsby": "0.5.12",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "react-test-renderer": "16.13.1",
    "sass": "1.26.11",
    "ts-jest": "26.4.1"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.(js|jsx)?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  }
}
