{
  "name": "kitten",
  "description": "",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "build:og-gen": "ts-node --project tsconfig.builder.json scripts/og-gen.ts",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "eslint": "eslint ./ --ext ts,tsx",
    "eslint:fix": "eslint ./ --ext ts,tsx --fix",
    "stylelint": "stylelint --config .stylelintrc.yml 'src/**/*.scss'",
    "stylelint:fix": "stylelint --config .stylelintrc.yml 'src/**/*.scss' --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@nekohack/footer-ui": "1.0.0",
    "@nekohack/normalize.css": "2.4.0",
    "@stripe/react-stripe-js": "1.6.0",
    "@stripe/stripe-js": "1.21.2",
    "dayjs": "1.10.7",
    "gatsby": "2.32.13",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-google-adsense": "1.1.3",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-react-intl": "1.3.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-robots-txt": "1.6.14",
    "gatsby-plugin-sass": "3.2.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-typescript": "2.12.1",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-yaml": "2.11.0",
    "nekohack-ui": "3.0.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-test-renderer": "16.14.0",
    "react-text-loop": "2.3.0",
    "styled-components": "5.3.3",
    "ts-node": "10.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/helper-create-regexp-features-plugin": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@nekohack/eslint-config-react": "0.5.0",
    "@swc/core": "1.2.118",
    "@types/jest": "26.0.24",
    "@types/react": "16.14.21",
    "@types/react-dom": "16.9.14",
    "@types/react-helmet": "6.1.3",
    "@types/react-test-renderer": "16.9.5",
    "@types/styled-components": "5.1.16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "1.14.0",
    "dotenv": "8.6.0",
    "eslint": "7.32.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "prettier": "2.5.1",
    "puppeteer": "12.0.1",
    "sass": "1.44.0",
    "stylelint": "14.1.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-recess-order": "2.6.0",
    "stylelint-config-standard": "23.0.0",
    "stylelint-scss": "4.0.0",
    "ts-jest": "26.5.6",
    "typescript": "4.5.2"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.(js|jsx)?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules",
      "__tests__/constants.ts",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  }
}
